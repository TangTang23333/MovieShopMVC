@model List<ApplicationCore.Models.PurchaseDetailModel>


<div>
    <div class="container-fluid" style="padding:80px;">
        <div class="row">
            <h3>Purchased</h3>
            @foreach (var movie in Model)
            {
                <div class="col-6 col-lg-3 col-sm-4 col-xl-2">

                    <partial name="_PurchasedMovieCardPartial" model="movie" />
                </div>
            }
        </div>
    </div>
</div>
